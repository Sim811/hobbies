<h1>Welcome <%= current_user.first_name %></h2>
<hr />
<%= link_to "Add Hobby", new_hobby_path  %>
  <h3>Your Hobbies</h3>
  <ul>
      <% @hobbies.each do |hobby| %>
        <li>
          <%= hobby.name %>
           <div>
